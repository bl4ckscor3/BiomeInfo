{
  "schemaVersion": 1,
  "id": "biomeinfo",
  "version": "${version}",

  "name": "BiomeInfo",
  "description": "Adds a configurable text to the top left corner (by default) saying what biome the player is in.",
  "authors": [
    "bl4ckscor3"
  ],
  "contact": {
    "homepage": "https://curseforge.com/minecraft/mc-mods/biomeinfo-fabric",
    "issues": "https://github.com/bl4ckscor3/BiomeInfo/issues",
    "sources": "https://github.com/bl4ckscor3/BiomeInfo/tree/1.18-fabric"
  },

  "license": "GPL-3.0",
  "icon": "assets/biomeinfo/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "bl4ckscor3.mod.biomeinfo.BiomeInfo"
    ],
    "modmenu": [
      "bl4ckscor3.mod.biomeinfo.ModMenuIntegration"
    ]
  },

  "depends": {
    "fabricloader": ">=0.7.0",
    "minecraft": "1.18.x",
    "java": ">=17",
    "cloth-config": ">=6.2.57"
  },
  "suggests": {
    "flamingo": "*"
  },

  "custom": {
    "modmenu:clientsideOnly": true
  }
}